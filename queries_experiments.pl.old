hsql3_2_1( [friends::friendsof as a,
            friends::friendsof as b,
            friends::friendsof as c],
         [a::nickname = 'Alice',   %retrieval
          b::nickname = 'Bob' ,    %retrieval
          c::nickname = 'Claire' , %retrieval
          a::friendship::with::friend = b::friendship::with::friend,  %correlation
          a::friendship::with::friend = c::friendship::with::friend], %correlation
         [a::nickname, a::friendship,
          b::nickname, b::friendship,
          c::nickname, c::friendship]
        ).

